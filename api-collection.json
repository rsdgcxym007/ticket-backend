          "            const endpoint = pm.info.requestName.toLowerCase();",
          "            if (endpoint.includes('order')) {",
          "                pm.globals.set('orderId', response.data.id);",
          "            } else if (endpoint.includes('user')) {",
          "                pm.globals.set('userId', response.data.id);",
          "            } else if (endpoint.includes('seat')) {",
          "                pm.globals.set('seatId', response.data.id);",
          "            } else if (endpoint.includes('zone')) {",
          "                pm.globals.set('zoneId', response.data.id);",
          "            } else if (endpoint.includes('referrer')) {",
